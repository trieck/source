# Insertion Sort; Sedgwick 6.2
#
def isort {

	var a = shift argv;
	var n = length a;	
	var i = n - 1;
	
	while (i > 0) {
		if (a[i-1] < a[i]) {
			var t = a[i-1];
			a[i-1] = a[i];
			a[i] = t;
		}	
		i = i - 1;
	}
	
	i = 1;
	while (i <= n - 1) {
		var j = i, t = a[i];
		while (j > 0 && t < a[j-1]) {
			a[j] = a[j-1]; 
			j = j - 1;
		}
		a[j] = t;
		i = i + 1;
	}
	
	return a;
}

